      [Newtonsoft.Json!6.0.3]
      [nubu!$(NubootPropNubuPackageVersion)]
      [OxyPlot.Core!2014.1.319.1]
      [OxyPlot.Wpf!2014.1.319.1]
      [pugixml!1.7]
      [python!3.2]
      [rapidjson!1.0.2]
      [rfa!8.0.1.2013]
      [ThomsonReuters.Desktop.SDK.UI.Signed!1.4.3]
      [tibcoems!8.1.0.3]
      [trek!4.0.38.434]
      [xerces!3.1.2.1]
