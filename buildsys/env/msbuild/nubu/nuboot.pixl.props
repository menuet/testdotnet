<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">


  <PropertyGroup>

    <NubootPropSourcePath Condition="'$(NubootPropSourcePath)' == ''">$(MSBuildThisFileDirectory)</NubootPropSourcePath>

    <NubootPropUnzipPath Condition="'$(NubootPropUnzipPath)' == ''">$(PixlPropOutputPath)\nuboot</NubootPropUnzipPath>

    <NubootPropNubuPackageVersion Condition="'$(NubootPropNubuPackageVersion)' == ''">1.1.0.4</NubootPropNubuPackageVersion>

    <NubootPropPackagesCachePath Condition="'$(NubootPropPackagesCachePath)' == ''">$(PixlPropNugetCachePath)</NubootPropPackagesCachePath>

    <NubootPropPackagesInstallPath Condition="'$(NubootPropPackagesInstallPath)' == ''">$(PixlPropNugetInstallPath)</NubootPropPackagesInstallPath>

    <NubootPropProfileName Condition="'$(NubootPropProfileName)' == ''">pixl</NubootPropProfileName>

    <NubootPropNugetPackagesList Condition="'$(NubootPropNugetPackagesList)' == ''">
