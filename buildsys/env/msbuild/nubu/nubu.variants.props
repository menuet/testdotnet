<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>

    <NubuPropNativeToolset Condition="'$(NubuPropNativeToolset)' == '' and '$(PlatformToolset)' == 'v100'">vs2010</NubuPropNativeToolset>
    <NubuPropNativeToolset Condition="'$(NubuPropNativeToolset)' == '' and '$(PlatformToolset)' == 'v110'">vs2012</NubuPropNativeToolset>
    <NubuPropNativeToolset Condition="'$(NubuPropNativeToolset)' == '' and '$(PlatformToolset)' == 'v120'">vs2013</NubuPropNativeToolset>
    <NubuPropNativeToolset Condition="'$(NubuPropNativeToolset)' == '' and '$(PlatformToolset)' == 'v140'">vs2015</NubuPropNativeToolset>
    <NubuPropNativeToolset Condition="'$(NubuPropNativeToolset)' == ''">vs2010</NubuPropNativeToolset>

    <NubuPropNativePlatform Condition="'$(NubuPropNativePlatform)' == '' and '$(Platform)' == 'Win32'">x86</NubuPropNativePlatform>
    <NubuPropNativePlatform Condition="'$(NubuPropNativePlatform)' == '' and '$(Platform)' == 'x64'">x64</NubuPropNativePlatform>
    <NubuPropNativePlatform Condition="'$(NubuPropNativePlatform)' == ''">x86</NubuPropNativePlatform>

    <NubuPropNativeConfiguration Condition="'$(NubuPropNativeConfiguration)' == '' and '$(Configuration)' == 'Debug'">d</NubuPropNativeConfiguration>
    <NubuPropNativeConfiguration Condition="'$(NubuPropNativeConfiguration)' == '' and '$(Configuration)' == 'Release'">r</NubuPropNativeConfiguration>
    <NubuPropNativeConfiguration Condition="'$(NubuPropNativeConfiguration)' == '' and '$(Configuration)' == 'Opt'">r</NubuPropNativeConfiguration>
    <NubuPropNativeConfiguration Condition="'$(NubuPropNativeConfiguration)' == ''">d</NubuPropNativeConfiguration>

    <NubuPropNativeVariant Condition="'$(NubuPropNativeVariant)' == ''">$(NubuPropNativeToolset)_$(NubuPropNativePlatform)_$(NubuPropNativeConfiguration)</NubuPropNativeVariant>

  </PropertyGroup>

  <PropertyGroup>

    <EikonPropToolset>$(NubuPropNativeToolset)</EikonPropToolset>
    <EikonPropPlatform>$(NubuPropNativePlatform)</EikonPropPlatform>
    <EikonPropConfiguration>$(NubuPropNativeConfiguration)</EikonPropConfiguration>
    <EikonPropVariant>$(NubuPropNativeVariant)</EikonPropVariant>

  </PropertyGroup>

</Project>